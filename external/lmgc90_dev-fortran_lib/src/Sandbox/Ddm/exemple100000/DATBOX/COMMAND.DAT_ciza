SUB Signorini Coulomb         : 
#
.doing Signorini Coulomb      : some label to draw attention
ECHO OFF
RECUP Rloc                    :
NLGS CHECK TYPE               :
Quad 0.1666D-04               : tol
    1.0D0                     : RELAX
EX PREP NLGS                  :
Stored_Delassus_Loops         :
>more                         :
  >iteration                  :
    EX ITER NLGS              :
  REPETE 10 FOIS              :
  DEPUIS >iteration           :
  AFTER ITER CHECK            :
  DISPLAY AFTER ITER CHECK    :
  IOK = FLAG 1                :
  INTO AFTER ITER CHECK       :
  STOP REPETE SI IOK = 0      :
REPETE 1000 FOIS               :
DEPUIS >more                  :
EX POST NLGS                  :
STOCK Rloc                    :
ECHO ON
